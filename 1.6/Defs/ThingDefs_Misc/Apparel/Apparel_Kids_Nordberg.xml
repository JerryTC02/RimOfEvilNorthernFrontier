<Defs>

  <ThingDef ParentName="ChildNordbergApparelMakeableBase">
    <defName>Apparel_ROE_NordbergBoy</defName>
    <label>Nordbergian Boys Parka</label>
    <description>A traditional parka worn by young boys in Nordberg, worn tight to allow them to run wild and play in the snow.</description>
    <techLevel>Medieval</techLevel>
    <recipeMaker>
      <skillRequirements>
        <Crafting>3</Crafting>
      </skillRequirements>
	  <displayPriority>50001</displayPriority>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Apparel/NordbergchildM/NordbergchildM</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>0.65</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>7500</WorkToMake>
      <MaxHitPoints>150</MaxHitPoints>
      <Mass>2.5</Mass>
      <Flammability>0.8</Flammability>
      <ArmorRating_Sharp>0.25</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.06</ArmorRating_Blunt>
      <ArmorRating_Heat>0.31</ArmorRating_Heat>
      <Insulation_Cold>48</Insulation_Cold>
      <Insulation_Heat>0</Insulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <equippedStatOffsets>
	  <MoveSpeed>0.15</MoveSpeed>
	  <RangedCooldownFactor>-0.1</RangedCooldownFactor>
    </equippedStatOffsets>
    <thingCategories>
      <li>ApparelArmor</li>
    </thingCategories>
    <costList>
	  <Leather_Heavy>45</Leather_Heavy>
    </costList>
    <apparel>
	  <gender>Male</gender>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
		<li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
	  <tags>
        <li>NordbergBoy</li>
      </tags>
      <wornGraphicPath>Things/Apparel/NordbergchildM/NordbergchildM</wornGraphicPath>
      <layers>
		<li>Shell</li>
      </layers>
    </apparel>
  </ThingDef>
  
