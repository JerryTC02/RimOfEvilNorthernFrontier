<?xml version="1.0" encoding="utf-8" ?>

<Defs>

  <ThingDef ParentName="ApparelNordbergMakeableBase">
    <defName>Apparel_ROE_NordbergA</defName>
    <label>Nordbergian Fur Parka</label>
    <description>A large fur parka worn by the people of Nordberg, incredibly warm and durable.</description>
    <techLevel>Medieval</techLevel>
    <recipeMaker>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
	  <displayPriority>60001</displayPriority>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Apparel/Nordbergvillager/NordbergvillagerA/NordbergvillagerA</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>1</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>9850</WorkToMake>
      <MaxHitPoints>195</MaxHitPoints>
      <Mass>4.1</Mass>
      <Flammability>0.8</Flammability>
      <ArmorRating_Sharp>0.25</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.06</ArmorRating_Blunt>
      <ArmorRating_Heat>0.31</ArmorRating_Heat>
      <Insulation_Cold>62</Insulation_Cold>
      <Insulation_Heat>0</Insulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <equippedStatOffsets>
		<WorkSpeedGlobal>0.1</WorkSpeedGlobal>
    </equippedStatOffsets>
    <thingCategories>
      <li>ApparelArmor</li>
    </thingCategories>
    <costList>
	  <Leather_Heavy>90</Leather_Heavy>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
		<li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
	  <tags>
        <li>Nordberg</li>
      </tags>
      <wornGraphicPath>Things/Apparel/Nordbergvillager/NordbergvillagerA/NordbergvillagerA</wornGraphicPath>
      <layers>
		<li>Shell</li>
      </layers>
    </apparel>
	<modExtensions>
     <li Class="taranchuk_apparelgraphics.ApparelExtension">
      <hideBody>False</hideBody>
      <femaleBody>Female</femaleBody>
      <maleBody>Male</maleBody>
     </li>
    </modExtensions>
  </ThingDef>
  
